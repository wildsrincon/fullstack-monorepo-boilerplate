{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "check-format": "npx prettier --check .",
    "check-lint": "npx eslint . --ext ts --ext tsx --ext js",
    "check-types": "npx tsc --pretty --noEmit",
    "dev": "vite",
    "fix": "npx eslint . --fix --ext ts --ext tsx --ext js",
    "format": "npx prettier --write .",
    "lint": "npx lint-staged",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest --run",
    "test-all": "pnpm run check-format && pnpm run check-lint && pnpm run check-types && pnpm run build",
    "test-watch": "vitest --watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@root/client": "workspace:*",
    "@root/server": "workspace:*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vitest": "^0.29.8"
  }
}
